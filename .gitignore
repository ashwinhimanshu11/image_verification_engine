# Ignore Python byte-code files, which are compiled files created by Python.
*.pyc
*.pyo
__pycache__

# Ignore environment-specific files
.env

# Ignore local configuration files
/.vscode
/.idea

# Ignore logs and runtime data
/logs
*.log

# Ignore files generated by package managers
/node_modules
/yarn.lock
/npm-debug.log*

# Ignore macOS system files
.DS_Store

# Ignore editor-specific files
*.suo
*.ntvs*

# Ignore backup files
*~
*.bak
